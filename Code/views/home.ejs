<% include chunks/head %>
<% include chunks/nav %>

<div class="container">
	<ol class="breadcrumb">
	  <li class="active">Home</li>
	</ol>
	<p>Home page for <%= username %>.</p>
</div>
<div class="container">
	<div class="col-xs-12 col-sm-6">
		<h4>Categories</h4>
		<div class="list-group">
			<% categories.forEach(function(item) { %>
			<a href="/home/<%= item._id %>" class="list-group-item">
				<span class="badge">Post Count?</span>
				<%= item.name %>
			</a>
			<% }); %>
		</div>
	</div>
</div>
<% include chunks/foot %>